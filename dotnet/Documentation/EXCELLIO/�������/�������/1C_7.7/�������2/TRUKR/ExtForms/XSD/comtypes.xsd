<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation>Загальні типи даних</xs:documentation>
	</xs:annotation>
	<!--Прості типи-->
	<!--Загальний тип "Код Стан документа (1, 2, 3) (1-звітний документ 2-новий звітний документ 3-уточнюючий документ)"-->
	<xs:simpleType name="DGSTAN">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[1,2,3]"/>
		 </xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код КОАТУУ (9-10 знаків)"-->
	<xs:simpleType name="DGKOATUU">
		<xs:restriction base="xs:unsignedLong">
			<xs:pattern value="[0-9]{9,10}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код від 1 до 4 знаків ДРФО, ознака типу документа для Реєстра накладних"-->
	<xs:simpleType name="DGOsp">
		<xs:restriction base="xs:integer">
		       <xs:pattern value="[1,2,3,4]"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код 0/1 для ДРФО"-->
	<xs:simpleType name="DGOzn">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0,1]"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код 1/2 для ДРФО"-->
	<xs:simpleType name="DGOzn12">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[1,2]"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Ознака доходу від 1 до 66 для ДРФО"-->
	<xs:simpleType name="DGOdoh">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="66"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код пільги (8 знаків)"-->
	<xs:simpleType name="DGCodPilg">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Місяць від 0 до 12"-->
	<xs:simpleType name="DGMonth0">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код МФО (6 знаків)"-->
	<xs:simpleType name="DGmfo">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код транспортного засобу (9 знаків)"-->
	<xs:simpleType name="DGTn-Zed">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код номера області"-->
	<xs:simpleType name="DGreg">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="29"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код зовнішньоекономічної діяльності"-->
	<xs:simpleType name="DGUKTZED">
		<xs:restriction base="xs:string">
			<xs:length value="10"/>
			<xs:pattern value="([0-9])([0-9])([0-9])([0-9])([0-9])([0-9])([0-9])([0-9])([0-9])([0-9])"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Число від 10000000 до 99999999"-->
	<xs:simpleType name="DGI8">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="10000000"/>
			<xs:maxInclusive value="99999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Число від 1000 до 99999"-->
	<xs:simpleType name="DGI5i">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="99999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Число від 0 до 999"-->
	<xs:simpleType name="DGI3i">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Число від 0 до 99"-->
	<xs:simpleType name="DGI2i">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "5 значне ціле додатне число"-->
	<xs:simpleType name="DGint5">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "2 значне ціле додатне число"-->
	<xs:simpleType name="DGint2">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "3 значне ціле додатне число"-->
	<xs:simpleType name="DGint3">
		<xs:restriction base="xs:unsignedLong">
			<xs:totalDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "КВЕД"-->
	<xs:simpleType name="DGkved">
		<xs:restriction base="xs:string">
			<xs:length value="7"/>
			<xs:pattern value="([0-9])([0-9])(\.)([0-9])([0-9])(\.)([0-9])"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Ознака"-->
	<xs:simpleType name="DGchk">
		<xs:restriction base="xs:int">
			<xs:enumeration value="1"/>
			<!--<xs:maxLength value="1"/>-->
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Місяць"-->
	<xs:simpleType name="DGMonth">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Квартал"-->
	<xs:simpleType name="DGKv">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Тип звітнoго періоду"-->
	<xs:simpleType name="DGPType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Рік народження"-->
	<xs:simpleType name="DGNYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1900"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Рік"-->
	<xs:simpleType name="DGYear">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1991"/>
			<xs:maxInclusive value="2099"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Назва підприємства"-->
	<xs:simpleType name="DGHNAME">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Адреса підприємства"-->
	<xs:simpleType name="DGHLOC">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Код міста"-->
	<xs:simpleType name="DGHZIP">
		<xs:restriction base="xs:unsignedLong">
			<xs:pattern value="([0-9]{5,5})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Електронна пошта"-->
	<xs:simpleType name="DGHEMAIL">
		<xs:restriction base="xs:string">
			<xs:pattern value="[^@]+@[^@]+"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Номер телефона"-->
	<xs:simpleType name="DGHTEL">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9 ()\-+\.,;]{4,}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Номер факса"-->
	<xs:simpleType name="DGHFAX">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9 ()\-+\.,;]{4,}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Назва ДПІ"-->
	<xs:simpleType name="DGHSTI">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 0 знаки після коми"-->
	<xs:simpleType name="DGdecimal0">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+(\.0{1,2}){0,1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 1 знаки після коми"-->
	<xs:simpleType name="DGdecimal1">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 2 знаки після коми"-->
	<xs:simpleType name="DGdecimal2">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 3 знаки після коми"-->
	<xs:simpleType name="DGdecimal3">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{1,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 4 знаки після коми"-->
	<xs:simpleType name="DGdecimal4">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 5 знаки після коми"-->
	<xs:simpleType name="DGdecimal5">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{5}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Числові дані 6 знаки після коми"-->
	<xs:simpleType name="DGdecimal6">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="\-{0,1}[0-9]+\.[0-9]{6}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "5-10-15 неоподатковуваних мінімумів "-->
	<xs:simpleType name="DG5-10-15nmg">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="10"/>
			<xs:enumeration value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ДРФО фізичної особи допускається числовий код, або № паспорта"-->
	<xs:simpleType name="DGDrfo">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10}|[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ЄДРПОУ (тiльки числа)"-->
	<xs:simpleType name="DGNLong">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ЄДРПОУ (ДРФО підприємця: числа, або № паспорта)"-->
	<xs:simpleType name="DGLong">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
			<xs:pattern value="([0-9]{5,10}|[АБВГДЕЄЖЗИІКЛМНОПРСТУФХЦЧШЩЮЯ]{2}[0-9]{6})"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип " Код HTINJ (Ідентифікаційний код платника згідно з ЄДРПОУ)"-->
	<xs:simpleType name="DGHTINJ">
		<xs:restriction base="DGNLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINF (Ідентифікаційний номер платника згідно з ДРФО)"-->
	<xs:simpleType name="DGHTINF">
		<xs:restriction base="DGLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINSD (Реєстраційний (обліковий) номер платника )"-->
	<xs:simpleType name="DGHTINSD">
		<xs:restriction base="DGLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HTINNR (Ідентифікаційний код представництвa нерезидента, що не має статусу юридичної особи)"-->
	<xs:simpleType name="DGHTINNR">
		<xs:restriction base="DGNLong"/>
	</xs:simpleType>
	<!--Загальний тип "Код HNPDV (Індивідуальний податковий номер платника ПДВ)"-->
	<xs:simpleType name="DGHNPDV">
		<xs:restriction base="DGLong12"/>
	</xs:simpleType>
	<!--Загальний тип "Індивідуальний податковий номер платника"-->
	<xs:simpleType name="DGHIPN">
		<xs:restriction base="DGLong12"/>
	</xs:simpleType>
	<!--Загальний тип "Код HDDGVSD (Дата договору про спільну (сумісну) діяльність)"-->
	<xs:simpleType name="DGHDDGVSD">
		<xs:restriction base="DGDate"/>
	</xs:simpleType>
	<!--Загальний тип "Код HNDGVSD (№ договору про спільну (сумісну) діяльність)"-->
	<xs:simpleType name="DGHNDGVSD">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Код HNSPDV (номер свідоцтва про реєстрацію платника ПДВ)"-->
	<xs:simpleType name="DGHNSPDV">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Код ДРФО директора"-->
	<xs:simpleType name="DGHKBOS">
		<xs:restriction base="DGDrfo"/>
	</xs:simpleType>
	<!--Загальний тип "ПІБ директора"-->
	<xs:simpleType name="DGHBOS">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Код ДРФО бухгалтера"-->
	<xs:simpleType name="DGHKBUH">
		<xs:restriction base="DGDrfo"/>
	</xs:simpleType>
	<!--Загальний тип "ПІБ бухгалтера"-->
	<xs:simpleType name="DGHBUH">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "ПІБ фізичної особи"-->
	<xs:simpleType name="DGHFO">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<!--Загальний тип "Індивідуальний податковий номер "-->
	<xs:simpleType name="DGLong12">
		<xs:restriction base="xs:unsignedLong">
			<xs:minInclusive value="1000"/>
			<xs:maxInclusive value="999999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Код ДПІ, використовується для обмеження коду області"-->
	<xs:simpleType name="DGsti">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип "Дата у форматі ддммрррр"-->
	<xs:simpleType name="DGDate">
		<xs:restriction base="xs:string">
			<xs:length value="8"/>
			<xs:pattern value="(((((0[1-9]|[1-2][0-9])(0(1|[3-9])|1[0-2]))|(30(0(1|[3-9])|1[0-2]))|(31(01|03|05|07|08|10|12)))(19|20)\d{2})|((0[1-9]|[1-2][0-9])02(19|20)[0-9](0|4))|((0[1-9]|[1-2][0-8]|19)02(19|20)[0-9](1|2|3|5|6|7|8|9)))"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Загальний тип атрибуту ROWNUM для вказання номеру строки в табличних даних-->
	<xs:simpleType name="rowInt">
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Опис комплексних типів які використовуються для контролю табличних даних-->
	<!--Загальний тип "Код ЕДРПОУ(ДРФО) в колонці"-->
	<xs:complexType name="TinColumn">
		<xs:simpleContent>
			<xs:extension base="DGLong">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані"-->
	<xs:complexType name="DecimalColumn">
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 0 знаки після коми"-->
	<xs:complexType name="Decimal0Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal0">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 1 знаки після коми"-->
	<xs:complexType name="Decimal1Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal1">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 2 знаки після коми"-->
	<xs:complexType name="Decimal2Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal2">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 3 знаки після коми"-->
	<xs:complexType name="Decimal3Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal3">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 4 знаки після коми"-->
	<xs:complexType name="Decimal4Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal4">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 5 знаки після коми"-->
	<xs:complexType name="Decimal5Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal5">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Числові дані 6 знаки після коми"-->
	<xs:complexType name="Decimal6Column">
		<xs:simpleContent>
			<xs:extension base="DGdecimal6">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Строкові дані"-->
	<xs:complexType name="StrColumn">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Дані типу Дата ддммрррр"-->
	<xs:complexType name="DateColumn">
		<xs:simpleContent>
			<xs:extension base="DGDate">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Індивідуальний податковий №"-->
	<xs:complexType name="IndTaxNumColumn">
		<xs:simpleContent>
			<xs:extension base="DGLong12">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!--Загальний тип "Цілі дані"-->
	<xs:complexType name="IntColumn">
		<xs:simpleContent>
			<xs:extension base="xs:integer">
				<xs:attribute name="ROWNUM" type="rowInt" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
