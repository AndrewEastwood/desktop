<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="ru">
	<xs:element name="_1CClientBankCheck">
		<xs:annotation>
			<xs:documentation>Квитанция на переданный пакет документов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ВерсияФормата" type="xs:string" fixed="1.00">
					<xs:annotation>
						<xs:documentation>Номер версии формата квитанции</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ИмяФайла" type="xs:string">
					<xs:annotation>
						<xs:documentation>Имя файла обмена на который возвращается квитанция</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Результат">
					<xs:annotation>
						<xs:documentation>Результат импорта документов</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Все приняты"/>
							<xs:enumeration value="Все отбракованы"/>
							<xs:enumeration value="Приняты частично"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:choice>
					<xs:element name="ОшибкаФормата" minOccurs="0" type="xs:string">
						<xs:annotation>
							<xs:documentation>Описание ошибки связанной с неверным форматом файла обмена</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ОшибкаСтруктуры" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Ошибки возникшие при проверке файла на соответствие схеме XSD</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Код" type="xs:int" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Код ошибки</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Строка" type="xs:unsignedInt">
									<xs:annotation>
										<xs:documentation>Номер строка в файле обмена</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Описание" type="xs:string">
									<xs:annotation>
										<xs:documentation>Текстовое описание ошибки</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ОшибкаИмпорта" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Ошибки возникшие при импорте документов в систему</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Номер" type="xs:unsignedInt">
									<xs:annotation>
										<xs:documentation>Номер ошибки</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Код" type="xs:int" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Код ошибки</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Строка" type="xs:unsignedInt">
									<xs:annotation>
										<xs:documentation>Номер строка в файле обмена</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Элемент" type="xs:NMTOKEN" minOccurs="0">
									<xs:annotation>
										<xs:documentation>"Элемент в котором возникла ошибка</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Описание" type="xs:string">
									<xs:annotation>
										<xs:documentation>Текстовое описание ошибки</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:choice>
				<xs:element name="Документ" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Список идентификаторов платежных документов и их статусов</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="Идентификатор" type="xs:string">
								<xs:annotation>
									<xs:documentation>Уникальный идентификатор документа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Статус">
								<xs:annotation>
									<xs:documentation>Результат импорта документа</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Принят"/>
										<xs:enumeration value="Отбракован"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
