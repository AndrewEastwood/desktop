<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="ru">
	<xs:element name="_1CClientBankExchange">
		<xs:annotation>
			<xs:documentation>Внутренний признак файла обмена</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ВерсияФормата" type="xs:string" fixed="1.00">
					<xs:annotation>
						<xs:documentation>Номер версии формата обмена</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Отправитель" type="xs:string">
					<xs:annotation>
						<xs:documentation>Программа-отправитель</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Получатель" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Программа-получатель</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ДатаСоздания" type="xs:date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата формирования файла</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ВремяСоздания" type="xs:time" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Время формирования файла</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ДатаНачала" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата начала интервала</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ДатаКонца" type="xs:date">
					<xs:annotation>
						<xs:documentation>Дата конца интервала</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="РасчСчет" type="accountNumber" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Расчетный счет организации</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Сальдо" type="moneySum">
					<xs:annotation>
						<xs:documentation>Исходящий остаток на конец интервала по всем передаваемым  расчетным счетам</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="СекцияДокумент" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Секция платежного документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:all>
							<xs:element name="ВидДокумента" type="suppDocTypes">
								<xs:annotation>
									<xs:documentation>Вид документа</xs:documentation>
								</xs:annotation>
							</xs:element>											
							<xs:element name="Номер" type="xs:string">
								<xs:annotation>
									<xs:documentation>Номер документа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Дата" type="xs:date">
								<xs:annotation>
									<xs:documentation>Дата документа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ДокументИД" type="xs:string">
								<xs:annotation>
									<xs:documentation>Идентификатор документа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Сумма" type="moneySum">
								<xs:annotation>
									<xs:documentation>Сумма платежа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="КодВалюты" type="xs:unsignedInt" fixed="980">
								<xs:annotation>
									<xs:documentation>Валюта платежа</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПлательщикСчет" type="accountNumber">
								<xs:annotation>
									<xs:documentation>Расчетный счет плательщика</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Плательщик" type="xs:string">
								<xs:annotation>
									<xs:documentation>Плательщик</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПлательщикОКПО" type="ОКПО">
								<xs:annotation>
									<xs:documentation>ОКПО плательщика (ЄДРПОУ)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПлательщикБанк" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Наименование банка плательщика</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПлательщикМФО" type="МФО">
								<xs:annotation>
									<xs:documentation>МФО банка плательщика</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПолучательСчет" type="accountNumber">
								<xs:annotation>
									<xs:documentation>Расчетный счет получателя</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Получатель" type="xs:string">
								<xs:annotation>
									<xs:documentation>Наименование получателя</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПолучательОКПО" type="ОКПО">
								<xs:annotation>
									<xs:documentation>ОКПО получателя (ЄДРПОУ)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПолучательБанк" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Наименование банка получателя</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ПолучательМФО" type="МФО">
								<xs:annotation>
									<xs:documentation>МФО банка получателя</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="НазначениеПлатежа" type="xs:string">
								<xs:annotation>
									<xs:documentation>Назначение платежа</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:annotation>
		<xs:documentation>Объявления типов данных, используемых в документе</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="accountNumber">
		<xs:annotation>
			<xs:documentation>Тип данных - цифровая строка от 5 до 14 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="5"/>
			<xs:maxLength value="14"/>
			<xs:pattern value="[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ОКПО">
		<xs:annotation>
			<xs:documentation>Тип данных - цифровая строка от 3 до 10 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:maxLength value="10"/>
			<xs:pattern value="[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="МФО">
		<xs:annotation>
			<xs:documentation>Тип данных - цифровая строка длиной 6 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="6"/>
			<xs:maxLength value="6"/>
			<xs:pattern value="[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="moneySum">
		<xs:annotation>
			<xs:documentation>Тип данных - денежная сумма в формате c 2 знаками после запятой</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="suppDocTypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Платежное поручение"/>
			<xs:enumeration value="Платежное требование-поручение"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>